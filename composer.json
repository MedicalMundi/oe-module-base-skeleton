{
  "name": "medicalmundi/oe-module-memu-skeleton",
  "type": "openemr-module",
  "description": "OpenEMR Custom Module Skeleton Starter Project",
  "keywords": [
    "openemr",
    "openemr-module"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Zerai Teclai",
      "email": "teclaizerai@gmail.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.1",
    "nyholm/psr7": "^1.4",
    "openemr/oe-module-installer-plugin": "^0.1.0",
    "symfony/event-dispatcher": "^4.4.0"
  },
  "conflict": {
    "openemr/openemr": "<6.0.0"
  },
  "require-dev": {
    "bamarni/composer-bin-plugin": "^1.4",
    "phpunit/phpunit": "~7.5.20"
  },
  "config": {
    "platform": {
      "php": "7.1.3"
    },
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "extra": {
    "bamarni-bin": {
      "target-directory": "tools"
    }
  },
  "autoload": {
    "psr-4": {
      "OpenEMR\\Modules\\Memu\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "OpenEMR\\Modules\\Memu\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "stable"
}
